package homework.model;

import java.util.function.IntConsumer;

public class Category {
	
	public Category(int id, String name) {
		this.Id = id;
		this.Name = name;
	}
	
	int Id;
	public int getId() {
		return Id;
	}
	public void setId(int id) {
		Id = id;
	}
	public String getName() {
		return Name;
	}
	public void setName(String name) {
		Name = name;
	}
	
	String Name;
	
}
